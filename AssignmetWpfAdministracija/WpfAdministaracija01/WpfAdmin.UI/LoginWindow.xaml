<Window x:Class="WpfAdmin.UI.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAdmin.UI"
        mc:Ignorable="d"
        Title="LoginWindow" Height="450" Width="400">
    <Grid>
        <TextBox x:Name="TextBoxUser" HorizontalAlignment="Left" Height="23" Margin="175,53,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="TextBoxPass" HorizontalAlignment="Left" Height="23" Margin="175,108,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="69,60,0,0" TextWrapping="Wrap" Text="Username:" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="69,115,0,0" TextWrapping="Wrap" Text="Password:" VerticalAlignment="Top"/>
        <Button Command="{Binding ButtonLogin_Click}" x:Name="ButtonLogin" Content="Login" HorizontalAlignment="Left" Margin="193,181,0,0" VerticalAlignment="Top" Width="75" Click="ButtonLogin_Click"/>

    </Grid>
</Window>
